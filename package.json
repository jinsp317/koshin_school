  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:dev": "npm run env:set -- dev && npm start -- -c",
    "start:prod": "npm run env:set -- prod && npm start -- -c",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "clean": "cd android && gradlew clean",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build",
    "ios": "react-native run-ios",
    "android": "react-native run-android --variant=release",
    "del:bundle": "del android/app/src/main/assets/index.android.bundle",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release && cd android && gradlew clean"
  },
